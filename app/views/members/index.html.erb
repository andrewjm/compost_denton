<% provide(:title, "All Members") %>
<h1>All Members</h1>

<div class="btn-group btn-group-justified" role="group" aria-label="...">
  <div class="btn-group" role="group">
    <button type="button" class="btn btn-lg btn-default" id="btn-first">First Name</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" class="btn btn-lg btn-default" id="btn-last">Last Name</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" class="btn btn-lg btn-default" id="btn-locale">Location</button>
  </div>
</div>

<ul class="users list-group">
  <% # For each @member render the _member.html.erb partial %>
  <% @members.each do |member| %>
    <%= render member %>
  <% end %>
</ul>
